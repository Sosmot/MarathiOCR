<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#85C1E9" />
            <gradient android:angle="-90" android:startColor="#EAEDED" android:endColor="#E8F8F5"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#85C1E9" />
            <solid android:color="#EAEDED"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="4dip" />
            <stroke android:width="1dip" android:color="#85C1E9" />
            <gradient android:angle="-90" android:startColor="#E8F8F5" android:endColor="#EAEDED" />
        </shape>
    </item>
</selector>